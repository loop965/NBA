    <select id="selectBrdCustomerInfoList" resultType="com.yf.producer.pojo.BrdCustomerInfo">
        SELECT
        a.CustomerPKId as "id",
        a.CustomerName,
        a.CustomerSex,
        a.NickName,
        a.Alias,
        a.Mobile,
        a.Personality,
        a.Birthday,
        a.CustomerStatus as "state",
        a.CustomerName as "createUser",
        a.CreateDateTime as "createTime",
        a.UpdatePerson as "updateUser",
        a.UpdateDateTime as "updateTime",
        a.Fashion,
        a.Contact,
    -- 	a.Source,
        b.FaceType,
        b.WaistLine,
        b.FaceComplexion,
        c.BodyType,
        c.BodyHeight,
        c.BodyWeight
    FROM
        cus_customer_base_info a
        LEFT JOIN cus_body_color b ON b.CustomerPKId = a.CustomerPKId
        LEFT JOIN cus_body_color_model c ON c.CustomerPKId = a.CustomerPKId
    </select>


